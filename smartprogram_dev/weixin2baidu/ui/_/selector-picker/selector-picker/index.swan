<view 
    id="{{onekitId}}"
    class="onekit-picker {{onekitClass}}"
    style="{{onekitStyle}}"
    bindtap="selector_show">
  <slot>
  </slot>
  </view>
  <bottom-view s-if="{{show}}" bindCancle="selector_cancle" bindConfirm="selector_confirm">
    <picker-view bindchange="selector_change" value="{{indexes}}">
      <picker-view-column>
        <view s-for="{{range}}" s-for-key="item">{{rangeKey?item[rangeKey]:item}}</view>
      </picker-view-column>
    </picker-view>
  </bottom-view>