<import src="../../../common/head.swan"/>
<import src="../../../common/foot.swan"/>

<view class="container">
  <template is="head" data="{{{title: 'createContext'}}}">
</template>

  <view class="page-body">
    <view class="page-section">
      <onekit-canvas Class="canvas-element" canvas-id="canvas"></onekit-canvas>
      <onekit-scroll-view Class="canvas-buttons" scroll-y="true">
        <block s-for="methods" s-for-item='method'>
          <onekit-button Class="canvas-button" bindtap="{{method}}">{{method}}</onekit-button>
        </block>
        <onekit-button Class="canvas-button" bindtap="toTempFilePath" type="primary">toTempFilePath</onekit-button>
      </onekit-scroll-view>
    </view>
  </view>

  <template is="foot">
</template>
</view>