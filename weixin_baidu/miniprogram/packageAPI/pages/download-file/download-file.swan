<import src="../../../common/head.swan"/>
<import src="../../../common/foot.swan"/>

<view class="container">
  <template is="head" data="{{{title: 'downloadFile'}}}">
</template>

  <view class="page-body">
    <onekit-image s-if="imageSrc" src="{{imageSrc}}" mode="center"></onekit-image>
    <block s-else>
      <view class="page-body-wording">
        <onekit-text Class="page-body-text">
          点击按钮下载服务端示例图片
        </onekit-text>
      </view>
      <view class="btn-area">
        <onekit-button bindtap="downloadImage" type="primary">下载</onekit-button>
      </view>
    </block>
  </view>

  <template is="foot">
</template>
</view>